{
  "name": "vite-component-tagger",
  "version": "1.0.0",
  "description": "A Vite plugin that tags JSX/TSX elements with component details for development and debugging",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "vite",
    "plugin",
    "react",
    "jsx",
    "tsx",
    "development",
    "debugging",
    "component-tagging"
  ],
  "author": "Nilesh Darji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nilesh9106/vite-tagger.git"
  },
  "bugs": {
    "url": "https://github.com/Nilesh9106/vite-tagger/issues"
  },
  "homepage": "https://github.com/Nilesh9106/vite-tagger#readme",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.9",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=16"
  }
}
